		Alumno =Entrada.nextLine();
		System.out.println ("numero de evaluacion a ingresar ");
		Evaluacion ="N".concat(Entrada.nextLine());
		System.out.println ("ingrese nota de evaluacion ");
		Nota =Entrada.nextLine();

		INFO = Curso+"_"+Alumno+"_"+Evaluacion+"_"+Nota;
		System.out.print(INFO);
    
   //Fin creacion ID
   
